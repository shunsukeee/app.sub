<h1>Books</h1>

<% @books.each do |book| %>
<th>Title</th>
<span><%= book.title %></span>
<th>Body</th>
<span><%= book.body %></span>
<% end %>
<%= form_for(@book) do |f| %>



<h2>New book</h2>
<h4>Title</h4>
<%= f.text_field :title %>
<h4>Body</h4>
<%= f.text_area :body %>

<%= f.submit 'Create Book' %>

<% end %>